page {
  padding: 40rpx;
  box-sizing: border-box;
  font-size: 36rpx;
}

.box {
  display: flex;
  flex-direction: column;
  color: #2f138d;
}

.box>view {
  margin-bottom: 40rpx;
}

.bigBox{
  width: 100%;
  justify-content: space-between;
}

.lrBox {
  display: flex;
  flex-direction: row;
  justify-content: between;
  align-items: center;
  color: #2f138d;
}

.textCenter {
  text-align: center;
}

.centerTit{
  width: 100%;
  height: 90rpx;
  background-color: #8DCDF9;
  color: #2f138d;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
}

.other{
  margin-top: 20rpx;
  /* line-height: 40rpx; */
}