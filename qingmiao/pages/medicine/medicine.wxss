.box{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 42rpx 30rpx;
}

.box view{
  width: 100%;
  height: 192rpx;
  line-height: 192rpx;
  text-align: center;
  background-color: #8DCDF9;
  border-radius: 20rpx;
  margin-bottom: 30rpx;
  color: #2f138d;
  font-size: 40rpx;
}