.box{
  display: flex;
  flex-direction: column;
  align-content: center;
  justify-content: center;
}

.list{
  display: flex;
  flex-direction: row;
  /* justify-content: center; */
  /* align-items: center; */
  padding: 30rpx 40rpx 40rpx 70rpx;
  border-bottom: 1px solid #ECE6E3;
  box-sizing: border-box;
}

.left{
  display: flex;
  flex-direction: column;
}

.title{
  font-size: 32rpx;
  color: #1F1B17;
}

.time{
  font-size: 28rpx;
  color: #AEA5A1;
  margin-top: 20rpx;
}

.right{
  width: 154px;
  height: 120px;
  box-sizing: border-box;
}

.right image{
  width: 100%;
  height: 100%;
}